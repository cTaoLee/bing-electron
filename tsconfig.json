{
  "files": [],
  "references": [{ "path": "./tsconfig.node.json" }, { "path": "./tsconfig.web.json" }],
  "include": [
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/renderer/src/**/*.svg",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    },
    "allowSyntheticDefaultImports": true
  }
}
